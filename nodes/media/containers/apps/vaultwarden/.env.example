# Service identity
SERVICE_NAME=vaultwarden

# Networking
VAULTWARDEN_PORT=8080

# Domain configuration
DOMAIN=https://vault.example.com

# Registration control
SIGNUPS_ALLOWED=false
INVITATIONS_ALLOWED=true

# Admin panel access token (generate with: openssl rand -base64 48)
ADMIN_TOKEN=CHANGE_ME_GENERATE_SECURE_TOKEN

# Timezone
TZ=America/Toronto

# Storage
VAULTWARDEN_DATA_DIR=/srv/vaultwarden/data